all: html

html: all-html

pdf: all-pdf

linkchecker: all-linkchecker

clean: all-clean

all-html:
	cd doc-Provisioning_Guide && $(MAKE) html

all-pdf:
	cd doc-Provisioning_Guide && $(MAKE) pdf

all-linkchecker:
	cd doc-Provisioning_Guide && $(MAKE) linkchecker

all-clean:
	cd doc-Provisioning_Guide && $(MAKE) clean
