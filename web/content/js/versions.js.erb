<%
  require 'bundler'
  require 'json'

  files = Dir[File.join('menu', '*.json')].sort_by do |name|
    version = File.basename(name, '.json')
    version = '9999' if version == 'nightly'
    Gem::Version.new(version)
  end

  versions = files.reverse.map do |file|
    data = JSON.parse(File.read(file))
    data['path'] = File.basename(file, '.json')
    data
  end
%>
const navVersions = <%= versions.to_json %>
